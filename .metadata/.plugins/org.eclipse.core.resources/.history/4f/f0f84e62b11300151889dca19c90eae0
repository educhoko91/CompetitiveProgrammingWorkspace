import java.util.ArrayList;
import java.util.HashMap;
import java.util.LinkedHashMap;
import java.util.Scanner;


public class Main {

	public static void main(String[] args) {
		Scanner in = new Scanner(System.in);
		int duration = 0;
		double payment = 0;
		double amount = 0;
		double loan = 0;
		double value = 0;
		double deprInit=0;
		int numDepr = 0;
		
		while((duration=in.nextInt())>0) {
			ArrayList<Double> depr = new ArrayList<Double>();
			
			payment = in.nextDouble();
			amount = in.nextDouble();
			numDepr = in.nextInt();
			value = amount;
			loan = amount;
			int aux =0 ;
			for(int i=0; i< numDepr; i++ ) {
				int num = in.nextInt();
				double auxDepr = in.nextDouble();
				while(aux < num) {
					
				}
				
			}
			
			value =amount * (1-deprInit);
			int i=0;
			int j=1;
			while(loan > value) {
				
				loan -= payment;
				while(!depr.containsKey(j))
					j--;
				 
				value = amount * (1-(deprInit - depr.get(j)));
				amount = value;
				j++;
				i++;
			}
			System.out.println(i);
			
			
		}
		

	}

}
