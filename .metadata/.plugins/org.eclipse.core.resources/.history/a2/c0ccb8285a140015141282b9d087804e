import java.util.Scanner;


public class Main {

	public static void main(String[] args) {
		Scanner in = new Scanner(System.in);
		int request = 0;
		int proporsal = 0;
		while((request=in.nextInt())!=0 && (proporsal=in.nextInt())!=0) {
			for(int i=0; i<request; i++) {
				in.nextLine();
			}
			
			String names[] = new String[proporsal];
			double values[] = new double[proporsal];
			int mets[] = new int[proporsal];
			double compli[] = new double[proporsal];
			for(int i=0; i< proporsal; i++) {
				names[i]=in.nextLine();
				values[i]=in.nextDouble();
				mets[i]=in.nextInt();
				for(int j=0; j< mets[i]; j++) {
					in.nextLine();
				}
				compli[i]=mets[i]/request;
			}
		}

	}

}
