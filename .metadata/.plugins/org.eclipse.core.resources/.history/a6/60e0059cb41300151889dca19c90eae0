import java.util.ArrayList;
import java.util.HashMap;
import java.util.LinkedHashMap;
import java.util.Scanner;


public class Main {

	public static void main(String[] args) {
		Scanner in = new Scanner(System.in);
		int duration = 0;
		double payment = 0;
		double amount = 0;
		double loan = 0;
		double value = 0;
		double deprInit=0;
		int numDepr = 0;
		
		while((duration=in.nextInt())>0) {
			ArrayList<Double> depr = new ArrayList<Double>();
			
			payment = in.nextDouble();
			amount = in.nextDouble();
			numDepr = in.nextInt();
			value = amount;
			loan = amount;
			int aux =0 ;
			double auxDeprAnt = 0;
			for(int i=0; i< numDepr; i++ ) {
				int num = in.nextInt();
				double auxDepr = in.nextDouble();
				while(aux < num) {
					depr.add(auxDepr);
					aux++;
				}
				aux++;
				depr.add(auxDepr);
			}
			
			value =amount * (1-(depr.get(0)-depr.get(1)));
			int i=0;
			int j=1;
			if(loan > value) {
				do {
					
					loan -=payment;
					value  *= (1-(depr.get(j)));
					amount = value;
					i++;
					if(j+1<depr.size())
						j++;
				}while(loan > value);
				System.out.println(i);
			}
			else {
				System.out.println(1);
			}
			
			
			
		}
		

	}

}
